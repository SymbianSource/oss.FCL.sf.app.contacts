/**
 *  Definition of Log length "No log" query.
 */
RESOURCE DIALOG r_ll_no_log_confirmation_query
    {
    flags = EGeneralQueryFlags;
    buttons = R_AVKON_SOFTKEYS_YES_NO;
    items =
        {
        DLG_LINE
            {
            type = EAknCtQuery;
            id = EGeneralQuery;
            control = AVKON_CONFIRMATION_QUERY
                {
                    layout = EConfirmationLayout;
                    label = qtn_logs_query_no_log;
                };
            }
        };
    }

