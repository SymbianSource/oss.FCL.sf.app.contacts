            name = "l";
            },
        LDAP_FIELDTYPE_MAP
            {
            // State, region
            CntFieldId = KUidContactFieldRegionValue;
            name = "st";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Country, actually country code
            CntFieldId = KUidContactFieldCountryValue;
            name = "c";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Country
            CntFieldId = KUidContactFieldCountryValue;
            name = "co";
            }
        };
    }

