            CntFieldId = KIntContactFieldVCardMapVOIP;
            name = "IPPhone";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Pager number, using vCard mapping
            CntFieldId = KIntContactFieldVCardMapPAGER;
            name = "pager";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Extension, where to map
            CntFieldId = 0;
            name = "nokiaExtensionNumber";
