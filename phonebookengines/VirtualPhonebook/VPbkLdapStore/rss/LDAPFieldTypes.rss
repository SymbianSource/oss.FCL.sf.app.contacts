// ----------------------------------------------------------------------------
// LDAP Directory attribute to contact model mapping, this part is customizable
// through external resource file
//
RESOURCE LDAP_FIELD_MAPPING_LIST r_ldap_field_mapping_list
    {
    items =
        {
        // Name characteristics
        LDAP_FIELDTYPE_MAP
            {
            // Common name, LDAP spesific
            name = "cn";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Distinguished name, LDAP spesific
            name = "dn";
            },
        LDAP_FIELDTYPE_MAP
            {
            // First name
            CntFieldId = KUidContactFieldGivenNameValue;
            name = "givenName";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Last name
            CntFieldId = KUidContactFieldFamilyNameValue;
            name = "sn";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Virtual phonebook maps this to nick name
            // CntFieldId = KUidContactFieldSecondNameValue;
            // Middle initial or second name
            // CntFieldId = KIntContactFieldVCardMapSECONDNAME;
            // Better mapping to virtual phonebook
            CntFieldId = KUidContactFieldAdditionalNameValue;
            name = "initials";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Display name, using vCard mapping type
            CntFieldId = KIntContactFieldVCardMapUnusedFN;
            name = "displayName";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Description to note
            CntFieldId = KUidContactFieldNoteValue;
            name = "description";
            },
            
        /* Picture, inetOrgPerson non-standard photo
        LDAP_FIELDTYPE_MAP
            {
            // Photo
            CntFieldId = KUidContactFieldPictureValue;
            name = "jpegPhoto";
            },
        */

        // Company characteristics
        LDAP_FIELDTYPE_MAP
            {
            // Title
            CntFieldId = KUidContactFieldJobTitleValue;
            name = "title";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Organization
            CntFieldId = KUidContactFieldCompanyNameValue;
            name = "o";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Organization - company
            CntFieldId = KUidContactFieldCompanyNameValue;
            name = "company";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Organization - company
            CntFieldId = KUidContactFieldCompanyNameValue;
            name = "nokiaInternalCompanyName";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Organizational unit, for search only
            CntFieldId = KUidContactFieldDepartmentNameValue;
            name = "ou";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Organizational unit, for search only
            CntFieldId = KUidContactFieldDepartmentNameValue;
            name = "department";
