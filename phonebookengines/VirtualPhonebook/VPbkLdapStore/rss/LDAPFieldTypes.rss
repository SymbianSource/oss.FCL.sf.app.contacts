            CntFieldId = KUidContactFieldEMailValue;
            name = "mail";
            },
            
        // Instant messaging
        // KUidContactFieldIMAddress
            
        // Url
        LDAP_FIELDTYPE_MAP
            {
            // Home page
            CntFieldId = KUidContactFieldUrlValue;
            name = "url";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Home page - Active Directory
            CntFieldId = KUidContactFieldUrlValue;
            name = "wWWHomePage";
            },
        LDAP_FIELDTYPE_MAP
            {
            // Home page - Nestcape, IBM, etc
            CntFieldId = KUidContactFieldUrlValue;
            name = "wWWURL";
