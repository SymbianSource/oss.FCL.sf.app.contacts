//  RESOURCE IDENTIFIER
NAME	PECR	// From Phonebook Extension Common Resources

#include <avkon.rh>
#include <eikon.rh>
#include <PbkExtCommon.hrh>
#include <Phonebook.loc>


//  RESOURCE DEFINITIONS 
// ---------------------------------------------------------

// Standard resource signature
// ---------------------------------------------------------
//
RESOURCE RSS_SIGNATURE { }

/**
 * Copy menu item
 */
RESOURCE MENU_PANE r_phonebook_copy_menu
	{
	items =
		{
		MENU_ITEM 
			{ 
            command = EPbkCmdCascadingCopy;
			cascade = r_phonebook_cascading_copy_menu;
			txt = qtn_phob_opt_copy;
			}
		};
	}

/**
 * Cascading copy menu
 * Contains place holder that is always dimmed.
 * Place holder is added because otherwise it would be impossible to
 * insert menu items to the first place.
 */
RESOURCE MENU_PANE r_phonebook_cascading_copy_menu
	{
	items = 
		{
        MENU_ITEM
            {
            command = EPbkCmdFirstCopyMenuItem;
            txt = "";
            flags = EEikMenuItemDimmed;
            }
		};
	}

// End of File
