        // System menu pane
        MENU_TITLE { menu_pane = r_phonebook_system_menu;               },
        // group members List View's menu pane with all items
        MENU_TITLE { menu_pane = r_pbk_groupmembers_sendcontactmenu;    },
	    MENU_TITLE { menu_pane = R_AVKON_MENUPANE_MARKABLE_LIST;        },
        MENU_TITLE { menu_pane = r_pbk_groupmembers_menu;               },
	MENU_TITLE { menu_pane = r_phonebook_poc_menu;		            },
        MENU_TITLE { menu_pane = r_phonebook_call_menu;		            },
        MENU_TITLE { menu_pane = r_phonebook_open_me_menu;		        }
        };
    }

