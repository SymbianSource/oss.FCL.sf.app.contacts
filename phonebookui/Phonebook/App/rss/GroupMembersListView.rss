    {
    items =
        {
        MENU_ITEM { command = EPbkCmdWrite;         txt = qtn_phob_opt_send; },
		MENU_ITEM { command = EPbkCmdRemoveFromFolder;	txt = qtn_phob_opt_entry_rem_from_grp; },
		MENU_ITEM { command = EPbkCmdBelongsToGroups;	txt = qtn_phob_opt_entry_is_in_grp; },
		MENU_ITEM { command = EPbkCmdFetchMembers;		txt = qtn_phob_opt_fetch_members; }
        };
    }

/**
 * Groups Members List View's menu pane, part 2
 */
