/**
 * Groups Members List View's main resource.
 */
RESOURCE AVKON_VIEW r_phonebook_groupmemberslist_appview
	{
	menubar = r_pbk_groupmembers_menubar;
	cba=R_AVKON_SOFTKEYS_OPTIONS_BACK;
	}

/**
 * Groups Members List View's tag group resource.
 */
RESOURCE TAB_GROUP r_pbk_group_members_tab_group
    {
    tab_width=EAknTabWidthWithOneTab;
    active=0;
    tabs=
        {
        TAB { 
            id = EPbkGroupMembersListViewId;
            }
        };
    }


// ---------------------------------------------------------
// Assign to group wait note
// ---------------------------------------------------------
// 
RESOURCE DIALOG r_qtn_phob_wnote_add_to_group
	{
	flags = EAknProgressNoteFlags;
    buttons = R_AVKON_SOFTKEYS_EMPTY;
    items =
		{
		DLG_LINE
			{
			type = EAknCtNote;
			id = EPbkNoteContactsAddToGroup;
			control = AVKON_NOTE 
				{ 
				layout = EProgressLayout;
				singular_label = qtn_gen_note_adding;
				};
			}
		};
	}


// ---------------------------------------------------------
// removing members from group wait note
// ---------------------------------------------------------
// 
RESOURCE DIALOG r_qtn_phob_wnote_rem_from_group
	{
	flags = EAknProgressNoteFlags;
    buttons = R_AVKON_SOFTKEYS_EMPTY;
    items =
		{
		DLG_LINE
			{
			type = EAknCtNote;
			id = EPbkNoteContactsRemoveFromGroup;
			control = AVKON_NOTE 
				{ 
				layout = EProgressLayout;
				singular_label = qtn_gen_note_removing;
                imagefile = AVKON_ICON_FILE;
                imageid = EMbmAvkonQgn_note_erased;
                imagemask = EMbmAvkonQgn_note_erased_mask;
				};
			}
		};
	}

