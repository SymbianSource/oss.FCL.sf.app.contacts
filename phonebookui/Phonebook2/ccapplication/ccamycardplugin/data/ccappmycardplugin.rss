#include <ecom/registryinfo.rh>
#include "ccappmycardpluginuids.hrh"

// ---------------------------------------------------------------------------
// theInfo
//
// ECom definitions for this plugin
// ---------------------------------------------------------------------------
//
RESOURCE REGISTRY_INFO theInfo
    {
    // UID for the DLL. See mmp files
    dll_uid = KCCAMyCardPluginDllUid;
    // Declare array of interface info
    interfaces = 
        {
        INTERFACE_INFO
            {
            // UID of interface that is implemented
            interface_uid = 0x20022FD0; //KCCAppViewPluginBaseInterfaceUID;

            implementations = 
                {
                IMPLEMENTATION_INFO
                    {
                    implementation_uid = KCCAMyCardPluginImplmentationUid;
                    version_no = 1;
                    display_name = "CCA MyCard view plugin";
                    default_data = "application/x-mycard";
                    opaque_data = "TABP=50\tMCD";
                    }
                };
            }
        };
    }
