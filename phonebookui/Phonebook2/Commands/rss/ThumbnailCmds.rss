#include "Pbk2UIControls.hrh"

// ---------------------------------------------------------
// Fetching a thumbnail to a contact wait note
// ---------------------------------------------------------
// 
RESOURCE DIALOG r_qtn_gen_note_fetching
	{
	flags = EAknWaitNoteFlags;
    buttons = R_AVKON_SOFTKEYS_EMPTY;
    items =
		{
		DLG_LINE
			{
			type = EAknCtNote;
			id = EPbk2NoteContactsFetching;
			control = AVKON_NOTE 
				{ 
				layout = EWaitLayout;
				singular_label = qtn_gen_note_fetching;
                animation = R_QGN_GRAF_WAIT_BAR_ANIM;
                imagefile = AVKON_ICON_FILE;
                imageid = EMbmAvkonQgn_note_copy;
                imagemask = EMbmAvkonQgn_note_copy_mask;
                };
			}
		};
	}
	
// ---------------------------------------------------------
// Field type for thumbnail image
// ---------------------------------------------------------
//
RESOURCE VPBK_FIELD_TYPE_MAPPING_LIST r_thumbnail_field_type
    {            
    items =
        {
        VPBK_FIELD_TYPE_MAPPING
            {
            versitProperty = VBK_FIELD_VERSIT_PROPERTY
                {
                name = EVPbkVersitNamePHOTO;
                };
            } 
        };
    }	

