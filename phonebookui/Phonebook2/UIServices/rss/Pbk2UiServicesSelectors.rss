#include <VPbkFieldType.rh>

/// This file contains selectors to be used in data save's
/// field type selection dialog. The selectors specify what field
/// types are shown in the query presented to user when she is saving
/// data to Phonebook.

// --------------------------------------------------------------------------
//  Selector for EGenericParamPhoneNumber data save
// --------------------------------------------------------------------------
//
RESOURCE VPBK_FIELD_TYPE_SELECTOR r_pbk2_aiw_phonenumber_selector
    {
    versitProperties =
        {
        VBK_FIELD_VERSIT_PROPERTY
            {
            name = EVPbkVersitNameTEL;
            },
        VBK_FIELD_VERSIT_PROPERTY
            {
            name = EVPbkVersitNameX;
            extensionName = "X-ASSISTANT-TEL";
            }
        };
    }

// --------------------------------------------------------------------------
//  Selector for EGenericParamPhoneNumber data save when VoIP is enabled
// --------------------------------------------------------------------------
//
RESOURCE VPBK_FIELD_TYPE_SELECTOR r_pbk2_aiw_phonenumber_selector_voip
    {
    versitProperties =
        {
        VBK_FIELD_VERSIT_PROPERTY
            {
            name = EVPbkVersitNameTEL;
            },
        VBK_FIELD_VERSIT_PROPERTY
            {
            name = EVPbkVersitNameX;
            extensionName = "X-ASSISTANT-TEL";
            },
        VBK_FIELD_VERSIT_PROPERTY
            {
            name = EVPbkVersitNameX;
            extensionName = "X-SIP";
            }
        };
    }

