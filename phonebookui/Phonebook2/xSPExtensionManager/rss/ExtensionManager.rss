    {
    // use resource format version 2 to enable ROM only plugins
    resource_format_version = RESOURCE_FORMAT_VERSION_2;
    dll_uid = KEXTENSIONMANAGERIMPLEMENTATIONUID;
    interfaces = 
        {
        INTERFACE_INFO
            {
            interface_uid = KPbk2UiExtensionInterfaceUID;
            implementations = 
                {
                IMPLEMENTATION_INFO
                    {
                    implementation_uid = KEXTENSIONMANAGERIMPLEMENTATIONUID;
                    version_no = 1;
                    display_name = "";
                    default_data = KPbk2UiExtensionDefaultDataString;
                    opaque_data = "z:\\resource\\ExtensionManagerRes.rsc";  // CSI: 25 #
                                rom_only = 0; // This is not a ROM only plugin
                    }
                };
            }
        };  
    }


